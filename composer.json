{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["micropython", "badge", "repository"],
    "authors": [
        {
            "name": "Anne Jan Brouwer",
            "email": "brouwer@annejan.com",
            "homepage": "https://annejan.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hatchery@badge.team",
        "isseus": "https://github.com/BadgeTeam/Hatchery/issues"
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "asbiin/laravel-webauthn": "^3.3",
        "bacon/bacon-qr-code": "^2.0",
        "bepsvpt/secure-headers": "^7.4",
        "calebporzio/sushi": "^2.4",
        "czproject/git-php": "^4.1",
        "darkaonline/l5-swagger": "^8.5",
        "intervention/image": "^2.5",
        "jorijn/laravel-security-checker": "^2.4",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "livewire/livewire": "^2.12",
        "pragmarx/google2fa-laravel": "^2.1",
        "pragmarx/recovery": "^0.2.1",
        "predis/predis": "^2.1",
        "seld/jsonlint": "^1.9",
        "sentry/sentry-laravel": "^3.3",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-html": "*",
        "spatie/laravel-sitemap": "^6.2",
        "symfony/yaml": "^5",
        "verschuur/laravel-robotstxt": "^5"
    },
    "require-dev": {
        "ext-dom": "*",
        "barryvdh/laravel-ide-helper": "^2.13",
        "codeception/codeception": "^5.0",
        "fakerphp/faker": "^1.21",
        "mockery/mockery": "^1.3",
        "nunomaduro/larastan": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.0",
	"pestphp/pest": "^2.0",
        "phan/phan": "^5.3",
        "phpunit/phpunit": "^10.0",
        "slevomat/coding-standard": "^8.11",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
	"classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
